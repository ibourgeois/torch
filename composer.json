{
  "name": "ibourgeois/torch",
  "description": "Laravel Docker CLI",
  "license": "MIT",
  "authors": [
    {
      "name": "Derek Bourgeois",
      "email": "derek@ibourgeois.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "iBourgeois\\Torch\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.0",
    "illuminate/support": "5.5.x|5.6.x"
  },
  "extra": {
    "laravel": {
      "providers": ["iBourgeois\\Torch\\TorchServiceProvider"],
      "aliases": {
        "Torch": "iBourgeois\\Torch\\Facade"
      }
    }
  }
}
