{
  "name": "ibourgeois/torch",
  "description": "Laravel Docker CLI",
  "keywords": ["torch", "cli", "docker", "laravel", "lumen", "ci", "devops", "deploy"],
  "license": "MIT",
  "authors": [
    {
      "name": "Derek Bourgeois",
      "email": "derek@ibourgeois.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "iBourgeois\\Torch\\": "src/"
    },
    "autoload-dev": {
      "psr-4": {
        "iBourgeois\\Torch\\Tests\\": "tests/"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.0",
    "dragonmantank/cron-expression": "^2.0",
    "guzzlehttp/guzzle": "^6.3",
    "illuminate/console": "^5.6",
    "illuminate/support": "5.5.x|5.6.x",
    "psr/log": "^1.0",
    "symfony/console": "^4.0",
    "symfony/process": "^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0|~5.0"
  },
  "extra": {
    "laravel": {
      "providers": ["iBourgeois\\Torch\\TorchServiceProvider"],
      "aliases": {
        "Torch": "iBourgeois\\Torch\\TorchFacade"
      }
    }
  },
  "config": {
    "sort-packages": true
  }
}
